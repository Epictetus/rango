# encoding: utf-8

class <%= @name.camel_case %>
  <% @models.each do |model| %>
  <% case @orm %>
  <% when "datamapper" %>
  class <%= model.camel_case %>
    include DataMapper::Resource
    property :title, String
  end

  <% when "sequel" %>
  class <%= model.camel_case < Sequel::Model %>
    # TODO: implement <%= model %> model
  end

  <% end %>
  <% end %>
end
